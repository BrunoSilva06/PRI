extends layout
block content
    .w3-container
        .w3-card-4
            table.w3-table-all
                tr
                    th Identificador
                    th Nota
                each i in person.Notas
                    tr
                        td= i.Indicador
                        td= i.Nota
    .w3-container
            form(method="get" action=`/Alunos`)
                button.w3-button.w3-round-xlarge.w3-right.w3-light-grey(type="submit") Voltar    